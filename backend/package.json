{
  "name": "collectivity",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "debug": "DEBUG=collectivity:* node --inspect ./bin/www",
    "clean": "rm -rf node_modules",
    "watch": "DEBUG=collectivity:* nodemon --inspect ./bin/www",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:all": "npm run migrate && npm run seed",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo"
  },
  "dependencies": {
    "body": "^5.1.0",
    "cloudinary": "^1.14.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^7.0.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "faker": "^4.1.0",
    "http-errors": "^1.7.2",
    "jimp": "^0.3.5",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.19",
    "lowdb": "^1.0.0",
    "mongoose": "^5.7.5",
    "morgan": "~1.9.0",
    "multer": "^1.4.1",
    "parser": "^0.1.4",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.33.0",
    "shortid": "^2.2.14",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "sequelize-cli": "^6.6.1",
    "typescript": "^5.2.2"
  }
}
